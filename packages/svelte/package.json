{
  "name": "@tolgee/svelte",
  "version": "5.16.2",
  "description": "Svelte implementation for Tolgee localization framework",
  "repository": "https://github.com/tolgee/tolgee-js",
  "homepage": "https://tolgee.io",
  "keywords": [
    "localization",
    "internationalization",
    "l10n",
    "i18n",
    "translation",
    "languages"
  ],
  "scripts": {
    "dev": "npm run develop",
    "develop": "sleep 5 && nodemon --exec svelte-package",
    "build": "svelte-kit sync && svelte-package",
    "clean": "rm -rf build build-e2e dist dist-e2e lib .svelte-kit package",
    "serve": "serve -s dist",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "jest src --collect-coverage",
    "lint": "prettier --cache --check . && eslint .",
    "format": "prettier --cache --write ."
  },
  "author": "JanCizmar",
  "license": "MIT",
  "peerDependencies": {
    "svelte": "^3.29.0 || ^4.0.0 || ^5.0.0-next.9"
  },
  "dependencies": {
    "@tolgee/web": "5.16.2"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "1.0.2",
    "@sveltejs/kit": "^1.20.4",
    "@sveltejs/package": "1.0.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/svelte": "^4.0.5",
    "@tolgee/format-icu": "5.16.2",
    "@tolgee/testing": "5.16.2",
    "@tsconfig/svelte": "^5.0.2",
    "@types/jest": "^29.5.9",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "28.1.2",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^5.0.0-next.9",
    "svelte-check": "^3.6.0",
    "svelte-jester": "^3.0.0",
    "svelte-preprocess": "^5.1.0",
    "svelte2tsx": "^0.6.25",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.1"
  },
  "type": "module",
  "publishConfig": {
    "directory": "./package",
    "access": "public"
  },
  "sideEffects": false
}